<h1>Users</h1>
<table class="table">
  <thead>
    <tr class="bg-white">
      <th scope="col">First Name</th>
      <th scope="col">Last Name</th>
      <th scope="col">Age</th>
      <th scope="col">Email</th>
    </tr>
  </thead>
  <tbody>
    {{#each users}}
        <tr class="bg-white">
            <td>{{this.firstname}}</td>
            <td>{{this.lastname}}</td>
            <td>{{this.age}}</td>
            <td>{{this.email}}</td>
        </tr>
    {{/each}}
</table>